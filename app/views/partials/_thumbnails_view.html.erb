<div class="col-md-offset-1 col-md-10 thumb-item-display">

  <% @squaresize = 3 if @squaresize.nil? %>
  <% @itemlist.each_slice(@squaresize) do |slice| %>
    <%= render partial: 'partials/thumbnail_row',locals: {slice: slice, thumbtype: @thumbtype, squaresize: @squaresize} %>
  <% end unless @itemlist.nil? %>
</div>
