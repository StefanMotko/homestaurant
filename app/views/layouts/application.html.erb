<!DOCTYPE html>
<html>
<head>
  <title>Homestaurant</title>
  <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div class="col-xs-2">
  <div class="col-xs-9 bg-success">
    <nav>
      <div class="row text-center"><%= link_to 'Home', '/' %></div>
      <div class="row text-center"><%= link_to 'Sign up', '/users/new' %></div>
      <div class="row text-center"><%= link_to 'Login', '/login' %></div>
      <div class="row text-center"><%= link_to 'About', '/about' %></div>
    </nav>
  </div>

</div>

<div class="col-xs-10">
  <%= yield %>
</div>

</body>
</html>
